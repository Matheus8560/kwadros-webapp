  arr[key].id = response.data._id;\n                        arr[key].uploaded = true;\n                    }\n                })\n                return arr;\n            });\n           \n\n        } catch(err){\n            setUploadFiles((state) =>  {\n                let arr = [...state];\n                state.map((index, key) => {\n                    if(index.id == files.id) arr[key].error = true;\n                })\n                return arr;\n            });\n            \n        }\n        \n    }\n\n    async function handleDelete(id){\n        try {\n            await api.delete(`/uploads/${id}`);\n            \n            let newArr;\n            setUploadFiles((state) =>  newArr = state.filter(file => file.id !== id) );\n            setUploadFiles(newArr);\n        } catch(err){\n            console.log(err.response.data.error)\n        }\n    }\n\n    return (\n        <Container>\n            <Header />\n            <Content>\n                <ContentDropzone>\n                    <Upload onUpload={e => handleUploads(e)} />\n                    <FileList files={uploadFiles} onDelete={handleDelete} />\n                </ContentDropzone>\n                \n                <PhotoFramesMenu />\n            </Content>\n            <Footer />\n            <GlobalStyle />\n        </Container>\n    );\n}\n","C:\\Users\\55219\\Desktop\\Aplications\\Kwadros\\webapp\\src\\reportWebVitals.js",[],"C:\\Users\\55219\\Desktop\\Aplications\\Kwadros\\webapp\\src\\styles\\global.js",[],"C:\\Users\\55219\\Desktop\\Aplications\\Kwadros\\webapp\\src\\styles\\styles.js",[],"C:\\Users\\55219\\Desktop\\Aplications\\Kwadros\\webapp\\src\\services\\api.js",[],"C:\\Users\\55219\\Desktop\\Aplications\\Kwadros\\webapp\\src\\components\\Upload\\index.js",[],"C:\\Users\\55219\\Desktop\\Aplications\\Kwadros\\webapp\\src\\components\\FileList\\index.js",["78"],"C:\\Users\\55219\\Desktop\\Aplications\\Kwadros\\webapp\\src\\components\\Upload\\styles.js",[],"C:\\Users\\55219\\Desk